<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xinshineng.information.dao.shaicha.ShaichaResultAdjustingDao">
	<resultMap id="domainMap" type="com.xinshineng.information.domain.shaicha.ShaichaResultAdjustingDO">
		<id column="t_adjusting_id" property="tAdjustingId"/>
		<result column="student_id" property="studentId"/>
		<result column="checkor_id" property="checkorId"/>
		<result column="adjusting_od" property="adjustingOd"/>
		<result column="adjusting_os" property="adjustingOs"/>
		<result column="adjusting_ou" property="adjustingOu"/>
		<result column="jj_od" property="jjOd"/>
		<result column="jj_os" property="jjOs"/>
		<result column="jj_ou" property="jjOu"/>
		<result column="check_date" property="checkDate"/>
		<result column="delete_flag" property="deleteFlag"/>
		<result column="identity_card" property="identityCard"/>
	</resultMap>
	<select id="get" resultMap="domainMap">
		select `t_adjusting_id`,`student_id`,`checkor_id`,`adjusting_od`,`adjusting_os`,`adjusting_ou`,`jj_od`,`jj_os`,`jj_ou`,`check_date`,`delete_flag`
		from s_result_adjusting where t_adjusting__id = #{value}
	</select>

	<select id="list" resultMap="domainMap">
		select `t_adjusting_id`,`student_id`,`checkor_id`,`adjusting_od`,`adjusting_os`,`adjusting_ou`,`jj_od`,`jj_os`,`jj_ou`,`check_date`,`delete_flag`,`identity_card`
		from s_result_adjusting

        <where>  
		  		  <if test="tAdjustingId != null and tAdjustingId != ''"> and t_adjusting_id = #{tAdjustingId} </if>
		  		  <if test="studentId != null and studentId != ''"> and student_id = #{studentId} </if>
		  		  <if test="checkorId != null and checkorId != ''"> and checkor_id = #{checkorId} </if>
		  		  <if test="adjustingOd != null and adjustingOd != ''"> and adjusting_od = #{adjustingOd} </if>
		  		  <if test="adjustingOs != null and adjustingOs != ''"> and adjusting_os = #{adjustingOs} </if>
		  		  <if test="adjustingOu != null and adjustingOu != ''"> and adjusting_ou = #{adjustingOu} </if>
		  		  <if test="jjOd != null and jjOd != ''"> and jj_od = #{jjOd} </if>
		  		  <if test="jjOs != null and jjOs != ''"> and jj_os = #{jjOs} </if>
		  		  <if test="jjOu != null and jjOu != ''"> and jj_ou = #{jjOu} </if>
		  		  <if test="checkDate != null and checkDate != ''"> and check_date = #{checkDate} </if>
		  		  <if test="deleteFlag != null and deleteFlag != ''"> and delete_flag = #{deleteFlag} </if>
		  		  <if test="identityCard != null and identityCard != ''"> and identity_card = #{identityCard} </if>
		  		  
		  		  <!-- <if test="startTime != null and startTime != ''"> and check_date &gt;= #{startTime} </if>
		  		  <if test="endTime != null and endTime != ''"> and check_date &lt;= #{endTime} </if> -->
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by ${sort} ${order}
            </when>
			<otherwise>
                order by t_adjusting_id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from s_result_adjusting
		 <where>  
		  		  <if test="tAdjustingId != null and tAdjustingId != ''"> and t_adjusting_id = #{tAdjustingId} </if>
		  		  <if test="studentId != null and studentId != ''"> and student_id = #{studentId} </if>
		  		  <if test="checkorId != null and checkorId != ''"> and checkor_id = #{checkorId} </if>
		  		  <if test="adjustingOd != null and adjustingOd != ''"> and adjusting_od = #{adjustingOd} </if>
		  		  <if test="adjustingOs != null and adjustingOs != ''"> and adjusting_os = #{adjustingOs} </if>
		  		  <if test="adjustingOu != null and adjustingOu != ''"> and adjusting_ou = #{adjustingOu} </if>
		  		  <if test="jjOd != null and jjOd != ''"> and jj_od = #{jjOd} </if>
		  		  <if test="jjOs != null and jjOs != ''"> and jj_os = #{jjOs} </if>
		  		  <if test="jjOu != null and jjOu != ''"> and jj_ou = #{jjOu} </if>
		  		  <if test="checkDate != null and checkDate != ''"> and check_date = #{checkDate} </if>
		  		  <if test="deleteFlag != null and deleteFlag != ''"> and delete_flag = #{deleteFlag} </if>
		  		  <if test="identityCard != null and identityCard != ''"> and identity_card = #{identityCard} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.xinshineng.information.domain.shaicha.ShaichaResultAdjustingDO" useGeneratedKeys="true" keyProperty="tAdjustingId">
		insert into s_result_adjusting
		(
			`student_id`, 
			`checkor_id`, 
			`adjusting_od`, 
			`adjusting_os`, 
			`adjusting_ou`, 
			`jj_od`, 
			`jj_os`, 
			`jj_ou`, 
			`check_date`, 
			`delete_flag`
		)
		values
		(
			#{studentId}, 
			#{checkorId}, 
			#{adjustingOd}, 
			#{adjustingOs}, 
			#{adjustingOu}, 
			#{jjOd}, 
			#{jjOs}, 
			#{jjOu}, 
			#{checkDate}, 
			#{deleteFlag}
		)
	</insert>
	 
	<update id="update" parameterType="com.xinshineng.information.domain.shaicha.ShaichaResultAdjustingDO">
		update s_result_adjusting
		<set>
			<if test="studentId != null">`student_id` = #{studentId}, </if>
			<if test="checkorId != null">`checkor_id` = #{checkorId}, </if>
			<if test="adjustingOd != null">`adjusting_od` = #{adjustingOd}, </if>
			<if test="adjustingOs != null">`adjusting_os` = #{adjustingOs}, </if>
			<if test="adjustingOu != null">`adjusting_ou` = #{adjustingOu}, </if>
			<if test="jjOd != null">`jj_od` = #{jjOd}, </if>
			<if test="jjOs != null">`jj_os` = #{jjOs}, </if>
			<if test="jjOu != null">`jj_ou` = #{jjOu}, </if>
			<if test="checkDate != null">`check_date` = #{checkDate}, </if>
			<if test="deleteFlag != null">`delete_flag` = #{deleteFlag}</if>
		</set>
		where t_adjusting_id = #{tAdjustingId}
	</update>
	
	<delete id="remove">
		delete from s_result_adjusting where t_adjusting_id = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from s_result_adjusting where t_adjusting_id in
		<foreach item="tAdjustingId" collection="array" open="(" separator="," close=")">
			#{tAdjustingId}
		</foreach>
	</delete>

</mapper>